{
  "name": "@aws-amplify/backend",
  "version": "0.2.0-alpha.8",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./test-utils": {
      "types": "./lib/types/test-utils/index.d.ts",
      "import": "./lib/esm/test-utils/index.js",
      "require": "./lib/cjs/test-utils/index.js"
    }
  },
  "types": "lib/types/index.d.ts",
  "scripts": {
    "build:types": "tsc --project tsconfig-types.json",
    "build:esm" : "tsc --build",
    "build:cjs": "tsc --project tsconfig-cjs.json",
    "update:api": "api-extractor run --local"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-amplify/backend-output-schemas": "^0.2.0-alpha.5",
    "@aws-amplify/backend-output-storage": "^0.1.1-alpha.3",
    "@aws-amplify/backend-secret": "^0.2.0-alpha.5",
    "@aws-amplify/platform-core": "^0.1.1-alpha.1",
    "@aws-amplify/plugin-types": "^0.2.0-alpha.9"
  },
  "peerDependencies": {
    "aws-cdk-lib": "~2.100.0",
    "constructs": "^10.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.119",
    "aws-lambda": "^1.0.7"
  }
}
