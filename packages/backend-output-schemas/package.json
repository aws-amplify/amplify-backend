{
  "name": "@aws-amplify/backend-output-schemas",
  "version": "0.2.0-alpha.6",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./auth": {
      "types": "./lib/types/auth/index.d.ts",
      "import": "./lib/esm/auth/index.js",
      "require": "./lib/cjs/auth/index.js"
    },
    "./graphql": {
      "types": "./lib/types/graphql/index.d.ts",
      "import": "./lib/esm/graphql/index.js",
      "require": "./lib/cjs/graphql/index.js"
    },
    "./platform": {
      "types": "./lib/types/platform/index.d.ts",
      "import": "./lib/esm/platform/index.js",
      "require": "./lib/cjs/platform/index.js"
    },
    "./storage": {
      "types": "./lib/types/storage/index.d.ts",
      "import": "./lib/esm/storage/index.js",
      "require": "./lib/cjs/storage/index.js"
    }
  },
  "scripts": {
    "build:types": "tsc --project tsconfig-types.json",
    "build:esm" : "tsc --build",
    "build:cjs": "tsc --project tsconfig-cjs.json",
    "update:api": "api-extractor run --local"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@aws-amplify/plugin-types": "0.2.0-alpha.9"
  },
  "peerDependencies": {
    "zod": "^3.21.4"
  }
}
