{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://amplify.aws/2024-02/outputs-schema.json",
  "title": "AWS Amplify Backend Outputs",
  "description": "Config format for Amplify Gen 2 client libraries to communicate with backend services.",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "$schema": {
      "description": "JSON schema",
      "type": "string"
    },
    "version": {
      "description": "Version of this schema",
      "const": "1"
    },
    "analytics": {
      "description": "Outputs manually specified by developers for use with frontend library",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "aws_region": {
          "description": "AWS Region of Amazon Pinpoint resources",
          "$ref": "#/$defs/aws_region"
        },
        "pinpoint_app_id": {
          "description": "Amazon Pinpoint App ID",
          "type": "string"
        }
      },
      "required": ["aws_region", "pinpoint_app_id"]
    },
    "api": {
      "description": "Outputs manually specified by developers for use with frontend library",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "endpoints": {
          "type": "array",
          "items": {
            "description": "API Gateway, HTTP API, or Function URLs",
            "type": "object",
            "additionalProperties": false,
            "properties": {
              "name": {
                "description": "API resource name",
                "type": "string"
              },
              "url": {
                "description": "Invoke endpoint of the API resource",
                "type": "string"
              },
              "aws_region": {
                "description": "AWS Region of the API resource",
                "$ref": "#/$defs/aws_region"
              },
              "authorization_types": {
                "type": "array",
                "items": {
                  "$ref": "#/$defs/amazon_api_gateway_authorization_type"
                }
              },
              "default_authorization_type": {
                "$ref": "#/$defs/amazon_api_gateway_authorization_type"
              }
            },
            "required": [
              "name",
              "url",
              "aws_region",
              "authorization_types",
              "default_authorization_type"
            ]
          }
        }
      },
      "required": ["endpoints"]
    },
    "auth": {
      "description": "Outputs generated from defineAuth",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "aws_region": {
          "description": "AWS Region of Amazon Cognito resources",
          "$ref": "#/$defs/aws_region"
        },
        "user_pool_id": {
          "description": "Cognito User Pool ID",
          "type": "string"
        },
        "user_pool_client_id": {
          "description": "Cognito User Pool Client ID",
          "type": "string"
        },
        "identity_pool_id": {
          "description": "Cognito Identity Pool ID",
          "type": "string"
        },
        "password_policy": {
          "description": "Cognito User Pool password policy",
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "min_length": {
              "type": "number",
              "minimum": 6,
              "maximum": 99
            },
            "require_numbers": {
              "type": "boolean"
            },
            "require_lowercase": {
              "type": "boolean"
            },
            "require_uppercase": {
              "type": "boolean"
            },
            "require_symbols": {
              "type": "boolean"
            }
          }
        },
        "identity_providers": {
          "description": "Identity providers set on Cognito User Pool",
          "type": "array",
          "items": {
            "type": "string"
          },
          "minItems": 0,
          "uniqueItems": true
        },
        "oauth_domain": {
          "description": "Cognito Domain used for identity providers",
          "type": "string"
        },
        "oauth_scope": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "minItems": 0,
          "uniqueItems": true
        },
        "oauth_redirect_sign_in": {
          "description": "URIs used to redirect after signing in using an identity provider",
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "oauth_redirect_sign_out": {
          "description": "URIs used to redirect after signing out",
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "oauth_response_type": {
          "type": "string",
          "enum": ["code", "token"]
        },
        "standard_attributes": {
          "description": "Cognito User Pool standard attributes",
          "type": "object",
          "additionalProperties": false,
          "propertyNames": {
            "$ref": "#/$defs/amazon_cognito_standard_attributes"
          },
          "patternProperties": {
            ".*": {
              "$ref": "#/$defs/amazon_cognito_standard_attributes_config"
            }
          }
        },
        "username_attributes": {
          "description": "Cognito User Pool username attributes",
          "type": "array",
          "items": {
            "type": "string",
            "enum": ["EMAIL", "PHONE"]
          }
        },
        "user_verification_mechanisms": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": ["EMAIL", "PHONE"]
          }
        },
        "unauthenticated_identities_enabled": {
          "type": "boolean",
          "default": true
        },
        "mfa_configuration": {
          "type": "string",
          "enum": ["NONE", "OPTIONAL", "REQUIRED"]
        },
        "mfa_methods": {
          "type": "array",
          "items": {
            "enum": ["SMS", "TOTP"]
          }
        }
      },
      "required": [
        "aws_region",
        "user_pool_id",
        "user_pool_client_id",
        "identity_pool_id"
      ]
    },
    "data": {
      "description": "Outputs generated from defineData",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "aws_region": {
          "$ref": "#/$defs/aws_region"
        },
        "url": {
          "description": "AppSync endpoint URL",
          "type": "string"
        },
        "model_introspection": {
          "description": "generated model introspection schema for use with generateClient",
          "type": "object"
        },
        "api_key": {
          "type": "string"
        },
        "default_authorization_type": {
          "$ref": "#/$defs/aws_appsync_authorization_type"
        },
        "authorization_types": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/aws_appsync_authorization_type"
          }
        },
        "conflict_resolution_mode": {
          "type": "string",
          "enum": ["AUTO_MERGE", "OPTIMISTIC_CONCURRENCY", "LAMBDA"]
        }
      },
      "required": [
        "aws_region",
        "url",
        "default_authorization_type",
        "authorization_types"
      ]
    },
    "geo": {
      "description": "Outputs manually specified by developers for use with frontend library",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "aws_region": {
          "description": "AWS Region of Amazon Location Service resources",
          "$ref": "#/$defs/aws_region"
        },
        "maps": {
          "description": "Maps from Amazon Location Service",
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "items": {
              "type": "array",
              "items": {
                "$ref": "#/$defs/amazon_location_service_config"
              }
            },
            "default": {
              "type": "string"
            }
          },
          "required": ["items", "default"]
        },
        "search_indices": {
          "description": "Location search (search by places, addresses, coordinates)",
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "items": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "default": {
              "type": "string"
            }
          },
          "required": ["items", "default"]
        },
        "geofence_collections": {
          "description": "Geofencing (visualize virtual perimeters)",
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "items": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "default": {
              "type": "string"
            }
          },
          "required": ["items", "default"]
        }
      },
      "required": ["aws_region"]
    },
    "storage": {
      "type": "object",
      "description": "Outputs generated from defineStorage",
      "additionalProperties": false,
      "properties": {
        "aws_region": {
          "$ref": "#/$defs/aws_region"
        },
        "bucket_name": {
          "type": "string"
        }
      },
      "required": ["aws_region", "bucket_name"]
    },
    "custom": {
      "description": "Outputs generated from backend.addOutput({ custom: <config> })",
      "type": "object"
    }
  },
  "required": ["version"],
  "$defs": {
    "aws_region": {
      "type": "string"
    },
    "amazon_cognito_standard_attributes": {
      "description": "Amazon Cognito standard attributes for users -- https://docs.aws.amazon.com/cognito/latest/developerguide/user-pool-settings-attributes.html",
      "type": "string",
      "enum": [
        "address",
        "birthdate",
        "email",
        "family_name",
        "gender",
        "given_name",
        "locale",
        "middle_name",
        "name",
        "nickname",
        "phone_number",
        "picture",
        "preferred_username",
        "profile",
        "sub",
        "updated_at",
        "website",
        "zoneinfo"
      ]
    },
    "amazon_cognito_standard_attributes_config": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "required": {
          "type": "boolean"
        }
      }
    },
    "aws_appsync_authorization_type": {
      "type": "string",
      "enum": [
        "AMAZON_COGNITO_USER_POOLS",
        "API_KEY",
        "AWS_IAM",
        "AWS_LAMBDA",
        "OPENID_CONNECT"
      ]
    },
    "amazon_api_gateway_authorization_type": {
      "$ref": "#/$defs/aws_appsync_authorization_type"
    },
    "amazon_location_service_config": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name": {
          "description": "Map resource name",
          "type": "string"
        },
        "style": {
          "description": "Map style",
          "type": "string"
        }
      }
    }
  }
}
