{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://amplify.aws/2024-02/outputs-schema.json",
  "title": "AWS Amplify (Gen 2) Backend Outputs",
  "description": "outputs generated by AWS Amplify (Gen 2) backends",
  "additionalProperties": false,
  "type": "object",
  "properties": {
    "_version": {
      "description": "Version of this schema",
      "const": "1"
    },
    "auth": {
      "description": "Outputs generated from defineAuth",
      "additionalProperties": false,
      "type": "object",
      "properties": {
        "aws_region": {
          "$ref": "#/$defs/aws_region"
        },
        "user_pool_id": {
          "description": "Cognito User Pool ID",
          "type": "string"
        },
        "user_pool_client_id": {
          "description": "Cognito User Pool Client ID",
          "type": "string"
        },
        "identity_pool_id": {
          "description": "Cognito Identity Pool ID",
          "type": "string"
        },
        "password_policy": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "min_length": {
              "type": "number",
              "minimum": 1
            },
            "require_numbers": {
              "type": "boolean"
            },
            "require_lowercase": {
              "type": "boolean"
            },
            "require_uppercase": {
              "type": "boolean"
            },
            "require_symbols": {
              "type": "boolean"
            },
            "require_digits": {
              "type": "boolean"
            }
          }
        },
        "identity_providers": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "minItems": 0,
          "uniqueItems": true
        },
        "oauth_domain": {
          "type": "string"
        },
        "oauth_scope": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "minItems": 0,
          "uniqueItems": true
        },
        "oauth_redirect_sign_in": {
          "type": "string"
        },
        "oauth_redirect_sign_out": {
          "type": "string"
        },
        "oauth_response_type": {
          "type": "string",
          "enum": ["code", "token"]
        },
        "standard_attributes": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "address": {
              "$ref": "#/$defs/amazon_cognito_standard_attributes_config"
            },
            "birthdate": {
              "$ref": "#/$defs/amazon_cognito_standard_attributes_config"
            },
            "email": {
              "$ref": "#/$defs/amazon_cognito_standard_attributes_config"
            },
            "family_name": {
              "$ref": "#/$defs/amazon_cognito_standard_attributes_config"
            },
            "gender": {
              "$ref": "#/$defs/amazon_cognito_standard_attributes_config"
            },
            "given_name": {
              "$ref": "#/$defs/amazon_cognito_standard_attributes_config"
            },
            "locale": {
              "$ref": "#/$defs/amazon_cognito_standard_attributes_config"
            },
            "middle_name": {
              "$ref": "#/$defs/amazon_cognito_standard_attributes_config"
            },
            "name": {
              "$ref": "#/$defs/amazon_cognito_standard_attributes_config"
            },
            "nickname": {
              "$ref": "#/$defs/amazon_cognito_standard_attributes_config"
            },
            "phone_number": {
              "$ref": "#/$defs/amazon_cognito_standard_attributes_config"
            },
            "picture": {
              "$ref": "#/$defs/amazon_cognito_standard_attributes_config"
            },
            "preferred_username": {
              "$ref": "#/$defs/amazon_cognito_standard_attributes_config"
            },
            "profile": {
              "$ref": "#/$defs/amazon_cognito_standard_attributes_config"
            },
            "sub": {
              "$ref": "#/$defs/amazon_cognito_standard_attributes_config"
            },
            "updated_at": {
              "$ref": "#/$defs/amazon_cognito_standard_attributes_config"
            },
            "website": {
              "$ref": "#/$defs/amazon_cognito_standard_attributes_config"
            },
            "zoneinfo": {
              "$ref": "#/$defs/amazon_cognito_standard_attributes_config"
            }
          }
        },
        "username_attributes": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": ["EMAIL", "PHONE"]
          }
        },
        "user_verification_mechanisms": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": ["EMAIL", "PHONE"]
          }
        },
        "mfa_configuration": {
          "type": "string",
          "enum": ["NONE", "OPTIONAL", "REQUIRED"]
        },
        "mfa_methods": {
          "type": "array",
          "items": {
            "enum": ["SMS", "TOTP"]
          }
        }
      }
    },
    "data": {
      "description": "Outputs generated from defineData",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "aws_region": {
          "$ref": "#/$defs/aws_region"
        },
        "url": {
          "description": "AppSync endpoint URL",
          "type": "string"
        },
        "model_introspection": {
          "description": "generated model introspection schema for use with generateClient",
          "type": "object"
        },
        "api_key": {
          "type": "string"
        },
        "default_authorization_type": {
          "type": "string"
        },
        "authorization_types": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "AMAZON_COGNITO_USER_POOLS",
              "API_KEY",
              "AWS_IAM",
              "AWS_LAMBDA",
              "OPENID_CONNECT"
            ]
          }
        }
      }
    },
    "storage": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "aws_region": {
          "$ref": "#/$defs/aws_region"
        },
        "name": {
          "type": "string"
        }
      }
    },
    "custom": {
      "description": "Outputs generated from backend.addOutput({custom: <config>})",
      "type": "object",
      "properties": {
        "Geo": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "aws_region": {
              "$ref": "#/$defs/aws_region"
            },
            "maps": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": ["VectorEsriStreets", "VectorEsriTopographic"]
              }
            }
          }
        }
      }
    }
  },
  "$defs": {
    "aws_region": {
      "type": "string",
      "enum": ["us-east-1", "us-east-2", "us-west-1", "us-west-2"]
    },
    "amazon_cognito_standard_attributes_config": {
      "type": "object",
      "properties": {
        "required": {
          "type": "boolean"
        }
      }
    }
  }
}
