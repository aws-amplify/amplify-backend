{
  "name": "@aws-amplify/client-config",
  "version": "1.8.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    },
    "./paths": {
      "types": "./lib/paths/index.d.ts",
      "import": "./lib/paths/index.js",
      "require": "./lib/paths/index.js"
    }
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "update:api": "api-extractor run --local"
  },
  "license": "Apache-2.0",
  "dependencies": {
